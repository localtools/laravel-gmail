{
  "name": "localtools/laravel-gmail",
  "description": "Gmail API package for Laravel",
  "keywords": [
    "api",
    "laravel",
    "Gmail"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Castro",
      "email": "danielcastro04@gmail.com",
      "homepage": "https://danielcastro.dev"
    },
    {
      "name": "Adrian Cerbaro",
      "email": "adrian@cerbaro.dev",
      "homepage": "https://cerbaro.dev"
    },
    {
      "name": "localtools",
      "email": "hebertcisco@outlook.com",
      "homepage": "https://github.com/localtools"
    }
  ],
  "require": {
    "php": "^7.4|^8.1",
    "ext-json": "*",
    "google/apiclient": "^2.14",
    "illuminate/auth": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/config": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/database": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/routing": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/session": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/support": "~5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
    "symfony/mime": "^5.4|^6.0"
  },
  "require-dev": {
    "google/apiclient-services": "^0.300.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^v4.18.0|^v5.20.0|^v6.25.0|^8.0",
    "phpunit/phpunit": "^9.3",
    "squizlabs/php_codesniffer": "~3.4"
  },
  "autoload": {
    "psr-4": {
      "Localtools\\LaravelGmail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Localtools\\LaravelGmail\\LaravelGmailServiceProvider"
      ],
      "aliases": {
        "LaravelGmail": "Localtools\\LaravelGmail\\Facade\\LaravelGmail"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}